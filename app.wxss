/**app.wxss**/
@import './assets/css/iconfont.wxss';
@font-face {
  font-family: 'myFont';
  src: url('data:font/woff2;charset=utf-8;base64,')
    format('woff2');
}

page {
  box-sizing: border-box;
  width: 100vw;
  height: 100vh;
  overflow: hidden;
}

view {
  box-sizing: border-box;
}

.iconfont {
  font-size: 100rpx;
}

.h1 {
  display: block;
  text-align: center;
  font-weight: bold;
  line-height: 1.5;
  font-size: 58rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}
.h2 {
  display: block;
  font-weight: bold;
  line-height: 1.5;
  font-size: 50rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.h3 {
  display: block;
  font-weight: bold;
  line-height: 1.5;
  font-size: 46rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.h4 {
  display: block;
  font-weight: bold;
  line-height: 1.5;
  font-size: 42rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.p {
  display: block;
  line-height: 2;
  font-size: 40rpx;
  text-align: justify;
}
.space {
  display: inline-block;
  width: 80rpx;
}
